#include <stdio.h>
#include <math.h>
#include <conio.h>
#include <malloc.h>


int n = 10, x = 0, we = 0, a = 0, r = 1, i;
int* list = (int*)malloc(n * sizeof(int));
int* list1 = (int*)malloc(n * sizeof(int));


int xx()
{
	while (r < n+1)
	{
		if (list[x] < list[r])
		{
			list1[a] = list[x];
			a++;
			x = r;
			r++;
		}
		else
		{
			while (list[x] > list[r] )
			{
				r++;
			}
			list1[a] = list[x];
			a++;
			x = r;
			r++;
		}
	}
	list = list1;
	while (we < a)
	{
		printf("%i ", list[we]);
		we++;
	}
	return 0;
}
int main()
{
	while (x < n)
	{
		printf("n = "); scanf_s("%i", &i);
		list[x] = i, x++;
	}
	printf("n = ");
	x = 0;
	xx();
	return 0;
}
